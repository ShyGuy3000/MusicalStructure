<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/playlists"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SuperMarioActivity"
            android:label="@string/mario"
            android:parentActivityName="com.example.android.musicalstructure.MainActivity"/>
        <activity
            android:name=".ZeldaActivity"
            android:label="@string/zelda"
            android:parentActivityName="com.example.android.musicalstructure.MainActivity"/>
        <activity
            android:name=".FinalFantasyActivity"
            android:label="@string/ff"
            android:parentActivityName="com.example.android.musicalstructure.MainActivity"/>
        <activity
            android:name=".MegaManActivity"
            android:label="@string/mm"
            android:parentActivityName="com.example.android.musicalstructure.MainActivity"/>
    </application>

</manifest>